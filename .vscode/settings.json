{
	"files.exclude": {
		"**/.vscode": true,
		"**/node_modules": true
	},
	"files.associations": {
		"*.jsx": "javascriptreact"
	},
	"editor.insertSpaces": true,
	"editor.detectIndentation": false,
	"editor.formatOnSave": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
  },
  "editor.tabSize": 2
}